int rec2(int a, int b, int c)
{
  if (a<=0 && b<=0 && c<=0)
    print(0);
  else {
    if (a>0) { print(a); a=a-1; }
    if (b>0) { print(b); b=b-1; }
    if (c>0) { print(c); c=c-1; }
    rec2(a,b,c);
  }
}

