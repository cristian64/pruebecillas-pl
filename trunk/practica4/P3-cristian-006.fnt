class Single
{
	public static void Main()
	{
		int a[][];
		a = new int[5][5];

		int contador;
		contador = 0;
		for (int i = 0; i < 5; i++)
		{
			for (int j = 0; j < 5; j++)
			{
				a[i][j] = contador++;
			}
		}

		for (int i = 0; i < 5; i++)
			for (int j = 0; j < 5; j++)
				System.Console.WriteLine(a[i][j]);
	}
}

